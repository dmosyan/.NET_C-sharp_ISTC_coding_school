<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="execute_Sync" Content="Normal Execute" HorizontalAlignment="Left" Height="54" Margin="63,30,0,0" VerticalAlignment="Top" Width="645" FontSize="28" Click="execute_Sync_Click"/>
        <Button x:Name="executeAsync" Content="Async Execute" HorizontalAlignment="Left" Height="64" Margin="63,103,0,0" VerticalAlignment="Top" Width="645" FontSize="28" Click="execute_Async_Click"/>
        <TextBlock x:Name="resultsWindow" HorizontalAlignment="Left" Height="190" Margin="63,200,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="645"/>


    </Grid>
</Window>
